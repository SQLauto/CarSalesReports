    <DataSet Name="CurrentMonthSales">
      <Query>
        <DataSourceName>CarSales_ODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportingYear">
            <Value>=Parameters!ReportingYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportingMonth">
            <Value>=Parameters!ReportingMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT		COUNT(C.Colour) AS NoSales
			,C.Colour

FROM		ODS.Colours C
			INNER JOIN ODS.Stock S
			ON C.ColourID = S.ColourID 
			INNER JOIN ODS.InvoiceLines L
			ON S.StockID = L.StockID 
			INNER JOIN ODS.Invoices I
			ON L.InvoiceID = I.InvoiceID

WHERE		YEAR(I.InvoiceDate) = @ReportingYear
			AND MONTH(I.InvoiceDate) = @ReportingMonth

GROUP BY	C.Colour</CommandText>
      </Query>
      <Fields>
        <Field Name="NoSales">
          <DataField>NoSales</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Colour">
          <DataField>Colour</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
