  <DataSets>
    <DataSet Name="CarColourSalesSinceYearStart">
      <Query>
        <DataSourceName>CarSales_ODS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ReportingYear">
            <Value>=Parameters!ReportingYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ReportingMonth">
            <Value>=Parameters!ReportingMonth.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
IF OBJECT_ID('tempdb..#Tmp_Output') IS NOT NULL DROP TABLE tempdb..#Tmp_Output

CREATE TABLE #Tmp_Output
(
NoSales INT NULL
,Colour VARCHAR(50) NULL
,SortDate DATE NULL
)

INSERT INTO #Tmp_Output
(
NoSales
,Colour
,SortDate
)

 

SELECT		NoSales, Colour, SortDate
FROM
		(
		SELECT		COUNT(C.Colour) AS NoSales
					,C.Colour
					,CAST(CAST(YEAR(I.InvoiceDate) AS CHAR(4)) + '-' + RIGHT('0' + CAST(MONTH(I.InvoiceDate) AS VARCHAR(2)),2) + '-01' AS DATE) AS SortDate

		FROM		ODS.Colours C
					INNER JOIN ODS.Stock S
					ON C.ColourID = S.ColourID 
					INNER JOIN ODS.InvoiceLines L
					ON S.StockID = L.StockID 
					INNER JOIN ODS.Invoices I
					ON L.InvoiceID = I.InvoiceID

		GROUP BY	C.Colour
					,CAST(CAST(YEAR(I.InvoiceDate) AS CHAR(4)) + '-' + RIGHT('0' + CAST(MONTH(I.InvoiceDate) AS VARCHAR(2)),2) + '-01' AS DATE)
					,YEAR(I.InvoiceDate)
					,MONTH(I.InvoiceDate)
) A

WHERE		SortDate &gt; DATEADD(mm,-12,CAST(CAST(@ReportingYear AS CHAR(4)) + '-' + RIGHT('0' + CAST(@ReportingMonth AS VARCHAR(2)),2) + '-01' AS DATE))
			AND SortDate &lt;= CAST(CAST(@ReportingYear AS CHAR(4)) + '-' + RIGHT('0' + CAST(@ReportingMonth AS VARCHAR(2)),2) + '-01' AS DATE)


SELECT * FROM #Tmp_Output ORDER BY SortDate</CommandText>
      </Query>
      <Fields>
